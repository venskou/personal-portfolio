{
  "name": "personal-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio website",
  "homepage": "https://github.com/venskou/personal-portfolio#readme",
  "bugs": {
    "url": "https://github.com/venskou/personal-portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venskou/personal-portfolio.git"
  },
  "license": "MIT",
  "author": {
    "name": "Uladzislau Vensko",
    "email": "ulad.vensko@gmail.com"
  },
  "scripts": {
    "build": "gulp build",
    "lint-js": "eslint \"src/**/*.js\"",
    "lint-js-fix": "eslint \"src/**/*.js\" --fix",
    "list-styles": "stylelint \"src/styles/**/*.scss\"",
    "list-styles-fix": "stylelint \"src/styles/**/*.scss\" --fix",
    "precommit": "lint-staged",
    "prettier": "prettier \"**/*.*\" --write",
    "server": "gulp server",
    "start": "gulp"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.*": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/styles/**/*.scss": [
      "stylelint \"src/styles/**/*.scss\" --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ],
  "dependencies": {
    "gumshoejs": "^5.1.2",
    "normalize.css": "^8.0.1",
    "smooth-scroll": "^16.1.0",
    "stickybits": "^3.6.7",
    "svg4everybody": "^2.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "browser-sync": "^2.26.7",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlhint": "^2.2.1",
    "gulp-imagemin": "^6.1.0",
    "gulp-modernizr": "^3.3.0",
    "gulp-npm-dist": "^1.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-pretty-html": "^2.0.10",
    "gulp-rename": "^1.4.0",
    "gulp-rimraf": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "prettier-plugin-packagejson": "^2.0.1",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.12.1"
  }
}
